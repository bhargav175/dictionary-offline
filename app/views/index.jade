html
	head
		if env != 'development'
			style
				include ../../static/style.css
	body
		#shell
		#app
			if env == 'development'
				script(src="http://localhost:2000/assets/vendor.js")
				script(src="http://localhost:2000/assets/bundle.js")
			else
				script(src="static/vendor.js")
				script(src="static/bundle.js")
